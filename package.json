{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:vmarcosp/react-webpack-boilerplate.git",
  "author": "Marcos <marcos@eventials.com>",
  "scripts": {
    "dev": "webpack-dashboard -- webpack-dev-server --hot --colors --mode development --config webpack/dev.js",
    "build": "webpack --mode production --config webpack/prod.js",
    "type-check": "tsc --project .",
    "type-check:watch": "yarn run type-check --watch",
    "tslint": "tslint -c tslint.json 'src/**/*.tsx'"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/ramda": "^0.26.7",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.14",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dashboard": "^2.1.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  }
}
